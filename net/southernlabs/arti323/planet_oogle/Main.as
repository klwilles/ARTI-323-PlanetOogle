package net.southernlabs.arti323.planet_oogle {		import flash.display.MovieClip;	import com.inruntime.utils.Global;	import net.southernlabs.arti323.planet_oogle.controllers.Router;	import net.southernlabs.arti323.planet_oogle.config.GameConfig;	import net.southernlabs.arti323.planet_oogle.models.GameAchievements;	import net.southernlabs.arti323.planet_oogle.views.fragments.Preloader;		public class Main extends MovieClip {				public var router:Router;		private var $:Global = Global.getInstance();				public var scenes_container:MovieClip;		public var preloader_container:MovieClip;				public function Main() {						scenes_container = new MovieClip();						preloader_container = new Preloader();			preloader_container.x = stage.stageWidth/2;			preloader_container.y = stage.stageHeight/2;						this.addChild(preloader_container);			this.addChild(scenes_container);						$.stage = this.stage;			$.score = new GameAchievements(); //persist in memory for now			$.preloader = preloader_container; // persist preloader			$.router = new Router(GameConfig.PLACE_INTRO, scenes_container);		}	}	}