package net.southernlabs.arti323.planet_oogle {		import flash.display.MovieClip;	import com.inruntime.utils.Global;	import net.southernlabs.arti323.planet_oogle.controllers.Router;	import net.southernlabs.arti323.planet_oogle.config.GameConfig;	import net.southernlabs.arti323.planet_oogle.models.GameAchievements;	import net.southernlabs.arti323.planet_oogle.views.fragments.Preloader;		public class Main extends MovieClip {				public var router:Router;		private var $:Global = Global.getInstance();				public var render_container:MovieClip;		public var preloader:MovieClip;				public function Main() {						render_container = new MovieClip();						preloader = new Preloader();			preloader.x = stage.stageWidth/2;			preloader.y = stage.stageHeight/2;						this.addChild(preloader);			this.addChild(render_container);						$.stage = this.stage;			$.score = new GameAchievements(); //persist in memory for now			$.preloader = preloader; // persist preloader			$.router = new Router(GameConfig.PLACE_INTRO, render_container);		}	}	}