package net.southernlabs.arti323.planet_oogle.controllers.places {		import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.display.Loader;	import flash.events.Event;	import flash.events.ProgressEvent;	import net.southernlabs.arti323.planet_oogle.config.GameConfig;	import net.southernlabs.arti323.controllers.ControllerInterface;	import com.inruntime.utils.Global;		public class Intro extends MovieClip implements ControllerInterface {		private var c:MovieClip;		private var $:Global = Global.getInstance();		public function Intro(){			trace(GameConfig.TITLE);		}		public function setRenderContainer(m:MovieClip):void {			c = m;		}				public function run():void {			var path:String = GameConfig.SCENE_PATH+"Intro.swf";			$.preloader.loadScene(path, onCompleteHandler);		}				public function end():void {						// delete stuff in container			trace("intro went poof");			while( c.numChildren > 0 ){				c.removeChildAt(0);			}			$.router.goto(GameConfig.PLACE_CRATER_GRAVE);					}		function onCompleteHandler(loadEvent:Event) {			trace("intro completely loaded");        	c.addChild(loadEvent.currentTarget.content);		}			}}